#!/bin/csh -f
  
rm -rf parsing_TW 

  
touch parsing_TW 

#echo `cat ./0_TW | sed '/台灣,#genre#/d'

cat 1_News | grep -v genre | cut -d "h" -f 1 > list

foreach chk ("`cat ./list`")
 echo $chk

 grep -i $chk ./TW > parsing_TW                                                                                                                                                          
end
  
  
